<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>STAT_DisplayOwnerName__c</fullName>
    <description>Field used to display the owner of the agreement. Used for email merge fields since apttus standard fields cannot be used as merge fields.</description>
    <externalId>false</externalId>
    <formula>IF(Owner:Queue.DeveloperName = &quot;Legal_Queue&quot;, Owner:Queue.QueueName, Owner:User.FirstName&amp;&quot; &quot;&amp;Owner:User.LastName)</formula>
    <label>DisplayOwnerName</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
